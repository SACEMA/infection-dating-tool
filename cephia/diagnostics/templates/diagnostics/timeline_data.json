[
    {
        "id":"{{subject.id}}",
        "title":"Subject - {{subject.subject_label}} Diagnostic Test Timeline",
        "description":"",
        "focus_date":"{{subject.subject_eddi.eddi|date:'Y-m-d'}}",
        "initial_zoom":"25",
        "image_lane_height":0,
        "events":[       
            {% for test in tests %}
            {
                "id":"{{test.id}}",
                "title": "{{test.test.name}}",
                "description":"Test Date: {{test.test_date}}<br/> Adjusted Date: {{test.adjusted_date}}<br/> Test Result: {{test.test_result}}",
                "startdate": "{{test.test_date|date:'Y-m-d'}}",
                "enddate": "{{test.adjusted_date|date:'Y-m-d'}}",
                "importance":"30",
                "date_display":"ye",
                "high_threshold":50,
                "link":"",
                "icon":{% if test.test_result == 'Negative' %}"circle_green.png"{% else %}"circle_red.png"{% endif %}
            }{% if not forloop.last %},{% endif %}
            {% endfor %}

        ],
        "legend": [
            {"title":"negative", "icon":"circle_green.png"},
            {"title":"positive", "icon":"circle_red.png"}
        ]
        
    }
]
